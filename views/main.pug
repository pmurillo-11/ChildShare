extends layout_common

block content
  script(type="module" src='./javascripts/main.js')

  nav.nav-extended(style="background-color: #006182;")
    .nav-wrapper(style="height: 100px;")
      img.brand-logo#Logo(src='images/childshare_3.png', alt='Logo')
      ul.right.hideonmed-and-down#nav-mobile(style="height: 100%;")
        li.navTitle.active(style="height: 100%;") childshare

  body
    .container
      btn.leftButton.col.s6.invisible.browser-default#newScriptButton(type="button" style="margin-bottom: 25px; margin-top: 25px;") NEW USER
      .col.s12.chips.chips-initial
      .row
        .col.s12#date
      .row
        #scriptList

  footer
    .footer-copyright(style="margin-top: 50px;")
      p.footerText 2019 ChildShare